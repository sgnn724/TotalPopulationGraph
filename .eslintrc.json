{
  "plugins": [
    "vue"
  ],
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "vue/component-name-in-template-casing": ["error", "PascalCase"],
    "vue/multi-word-component-names": "off",
    "prettier/prettier": ["error", { "singleAttributePerLine": true }],
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "no-console": "warn",
    "eqeqeq": "error"
  }
}
